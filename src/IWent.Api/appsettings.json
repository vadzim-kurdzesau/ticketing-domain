{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Caching": {
    "SlidingExpiration": "0.00:00:15"
  },
  "ConnectionStrings": {
    "Default": "{CONNECTION_STRING}",
    "Cache": "{CONNECTION_STRING}"
  },
  "Email": {
    "Host": "{EMAIL_HOST}",
    "Port": 0,
    "Username": "{USERNAME}",
    "Password": "{PASSWORD}"
  }
}
